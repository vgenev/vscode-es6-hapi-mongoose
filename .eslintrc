{
    "rules": {
        // "indent": [
        //     2
        // ],
        "quotes": [
            1,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-console" : [
            0
        ],
        "no-used-vars" : 0
    },
    "env": {
        "es6": true,
        "node": true,
        "browser": false
    },
    "ecmaFeatures" : {
      "modules" : true  
    },
    "extends": "eslint:recommended"
}